{
  "authenticationMethod": "none",
  "welcomeFile": "index.html",
  "routes": [
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/odata/v4/$1", 
      "destination": "srv-api",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)$",
      "target": "/$1",
      "localDir": "resources",
      "authenticationType": "none"
    }
  ]
}