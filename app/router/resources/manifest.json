{
  "_version": "1.76.0",
  "sap.app": {
    "id": "equipment.app.fiori",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [""]
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.19.1",
      "toolsId": "43edb04f-881b-4de8-8054-dd3dd00364af"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/EquipmentService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "equipment.app.fiori.i18n.i18n",
          "supportedLocales": [""]
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "EquipmentsList",
          "target": "EquipmentsList"
        },
        {
          "pattern": "Equipments({key}):?query:",
          "name": "EquipmentsObjectPage",
          "target": "EquipmentsObjectPage"
        },
        {
          "pattern": "shopfloor-view",
          "name": "ShopfloorViewRoute",
          "target": "ShopfloorViewTarget"
        }
      ],
      "targets": {
        "EquipmentsList": {
          "type": "Component",
          "id": "EquipmentsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Equipments",
              "variantManagement": "Page",
              "navigation": {
                "Equipments": {
                  "detail": {
                    "route": "EquipmentsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "EquipmentsObjectPage": {
          "type": "Component",
          "id": "EquipmentsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Equipments"
            }
          }
        },
        "ShopfloorViewTarget": {
          "viewName": "equipment.app.fiori.ext.view.ShopfloorView",
          "viewId": "ShopfloorViewView",
          "viewLevel": 1,
          "type": "View"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
